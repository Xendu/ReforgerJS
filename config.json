{
  "server": {
    "id": 1,
    "name": "SERVER NAME",
    "host": "xxx.xxx.xxx.xxx",
    "queryPort": 1111,
    "rconPort": 1111,
    "rconPassword": "PASSWORD",
    "reloadCommandsOnStartup": true,
    "logReaderMode": "tail",
    "filename": "console.log",
    "backfill": true,
    "logDir": "CC:/path/to/reforger/log/folder",
    "sftp": {
      "host": "xxx.xxx.xxx.xxx",
      "port": 21,
      "username": "SFTP Username",
      "password": "SFTP Password"
    },
    "ftp": {
      "host": "xxx.xxx.xxx.xxx",
      "port": 21,
      "user": "FTP Username",
      "password": "FTP Password"
    }
  },
  "customParsers": {
    "rjs-logging": {
      "enabled": true,
      "logDir": "/path/to/RJSLogging/events"
    }
  },
  "consoleLogLevel": "info",
  "outputLogLevel": "info",
  "connectors": {
    "discord": {
      "token": "",
      "clientId": "",
      "guildId": ""
    },
    "mysql": {
      "enabled": false,
      "host": "mysql-host",
      "port": 3306,
      "username": "reforgerjs",
      "password": "password",
      "database": "reforgerjs",
      "dialect": "mysql"
    },
    "battlemetrics": {
      "enabled": true,
      "orgID": "Your Org ID",
      "serverID": "Your Server ID",
      "token": "Your BattleMetrics API Token",
      "listID": "Your Ban List ID"
    }
  },
  "roles": {
    "roleName": "discord RoleID",
    "roleName1": "discord RoleID",
    "roleName2": "discord RoleID",
    "roleName3": "discord RoleID"
  },
  "roleLevels": {
    "1": ["roleName", "roleName1"],
    "2": ["roleName2"],
    "3": ["roleName3"]
  },
  "commands": [
    {
      "command": "whois",
      "enabled": false,
      "commandLevel": 3
    },
    {
      "command": "stats",
      "enabled": false,
      "commandLevel": 3,
      "servers": 1,
      "statsTable": "Same_as_DBLogStats_Plugin"
    },
    {
      "command": "voteinfo",
      "commandLevel": 3,
      "enabled": false
    },
    {
      "command": "rcon",
      "enabled": false,
      "commandLevel": 3,
      "ban": 3,
      "shutdown": 3,
      "restart": 3,
      "kick": 3
    },
    {
      "command": "killhistorywcs",
      "enabled": false,
      "commandLevel": 3
    },
    {
      "command": "messagehistorywcs",
      "enabled": false,
      "commandLevel": 3
    }
  ],
  "plugins": [
    {
      "plugin": "LogVoteKickVictim",
      "enabled": false,
      "channel": ""
    },
    {
      "plugin": "LogVoteKickStart",
      "enabled": false,
      "channel": ""
    },
    {
      "plugin": "DBLog",
      "enabled": false,
      "interval": 5
    },
    {
      "plugin": "VoteLogs",
      "enabled": false
    },
    {
      "plugin": "DBLogStats",
      "enabled": false,
      "interval": 5,
      "path": "C:/path/to/saves/profile/.save/playersave",
      "tableName": ""
    },
    {
      "plugin": "SeedTrackerBasic",
      "enabled": false,
      "interval": 5,
      "seedStart": 4,
      "seedEnd": 40
    },
    {
      "plugin": "AltChecker",
      "enabled": false,
      "channel": "",
      "logAlts": true,
      "logOnlyOnline": false
    },
    {
      "plugin": "ServerStatus",
      "enabled": false,
      "channel": "",
      "messageID": "",
      "interval": 1,
      "showFPS": true,
      "showMemoryUsage": false,
      "discordBotStatus": true,
      "embed": {
        "title": "Arma Reforger Server Status",
        "color": "#00FF00",
        "footer": "ReforgerJS",
        "thumbnail": false,
        "thumbnailURL": "https://IMAGE_URL_HERE.png"
      }
    },
    {
      "plugin": "RJS_LogChat",
      "requiredMods": "RJS Logging",
      "enabled": false,
      "channel": "your-discord-channel-id-here"
    },
    {
      "plugin": "RJS_LogTK",
      "requiredMods": "RJS Logging",
      "enabled": false,
      "channel": "your-discord-channel-id-here"
    },
    {
      "plugin": "RJS_DBEvents",
      "requiredMods": "RJS Logging",
      "enabled": false
    },
    {
      "plugin": "RJS_DBLog",
      "requiredMods": "RJS Logging",
      "enabled": false
    },
    {
      "plugin": "RJS_TK_Punishments",
      "enabled": false,
      "requiredMods": "GM Tools, RJS Logging",
      "teamkillKickLimit": 4,
      "logKicks": true,
      "logKickMessage": "Player was kicked for teamkilling\nReforgerJS Automated System",
      "warnEveryTK": true,
      "warningMessage": "Watch your fire, Teamkilling will result in your removal from the server",
      "enableBMBans": true,
      "banKickThreshold": 2,
      "banKickLimit": 14,
      "BanDuration": 24,
      "BanReason": "Intentional Teamkilling - banned for {{duration}} - appeal at https://discord.gg/LinkToDiscord",
      "BanMessage": "Banned for Teamkilling. ReforgerJS Automated System",
      "BanAutoAddEnabled": false,
      "BanNativeEnabled": false,
      "BanOrgWide": true
    },
    {
      "plugin": "BMDevTest",
      "enabled": false,
      "testUUID": "Test player UUID",
      "testFlags": false,
      "testFlagID": "Test FlagID",
      "testNotes": false,
      "testNote": "TestNoteMessage",
      "testBans": false,
      "testBanDuration": 24,
      "testBanReason": "Test- banned for {{duration}} - appeal at https://discord.gg/test",
      "testBanMessage": "BanMessageNotes",
      "testBanAutoAddEnabled": false,
      "testBanNativeEnabled": false,
      "testBanOrgWide": true,
      "testBanID": "banIDHere",
      "testEditBan": false,
      "testBanEditMessage": "BanMessageNotes - Edited",
      "testRemoveBan": false
    }
  ],
  "github": {
    "owner": "ZSU-GG-Reforger",
    "repo": "ReforgerJS"
  }
}
